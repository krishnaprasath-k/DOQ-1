{
  "name": "medical-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "analyze": "ANALYZE=true npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "start:prod": "NODE_ENV=production npm run start",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:vercel": "vercel --prod",
    "deploy:docker": "docker build -t doq-medical-ai .",
    "docker:run": "docker run -p 3000:3000 doq-medical-ai",
    "docker:compose": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose -f docker-compose.prod.yml down",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "clean": "rm -rf .next node_modules/.cache",
    "postinstall": "npm run type-check"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.23.2",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tabler/icons-react": "^3.34.0",
    "@vapi-ai/web": "^2.3.8",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.0.1",
    "drizzle-orm": "^0.44.2",
    "lucide-react": "^0.525.0",
    "moment": "^2.30.1",
    "motion": "^12.23.0",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "openai": "^5.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
